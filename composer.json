{
    "name": "nbish11/meek-routing",
    "description": "A simple framework for routing URLs in PHP.",
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "zendframework/zend-diactoros": "^1.3",
        "phpmd/phpmd": "^2.4",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Bishop",
            "email": "nbish11@hotmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "psr/http-message": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Meek\\Routing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meek\\Routing\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    }
}
